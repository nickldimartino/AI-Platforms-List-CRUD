<%- include("../partials/header"); %>

<!-- POST request to add new company info to the associated AI Platform -->
<form id="new-form-platform" action="/companies/<%= platformId %>" method="POST">
    <label>Company Name:
        <input type="text" name="name" >
    </label><br>
    <label>Year Created:
        <input type="text" name="yearCreated">
    </label><br>
    <label>Website:
        <input type="text" name="website">
    </label><br>
    <input type="submit" value="Confirm Edit">
</form>

<%- include("../partials/footer"); %>